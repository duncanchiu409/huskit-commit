{
  "name": "huskit-commit",
  "version": "1.0.6",
  "description": "[LLM Automation] A npm husky package for writing commit with LLM",
  "main": "dist/index.js",
  "repository": {
    "url": "https://github.com/duncanchiu409/huskit-commit.git"
  },
  "scripts": {
    "prepare": "husky",
    "prerelease": "pnpm run build",
    "release": "standard-version -t build_ && git push --follow-tags origin main && pnpm publish",
    "prettier": "prettier --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "eslint": "eslint \"**/*.+(ts|tsx)\"",
    "build": "rm -rf dist/ && tsc --project tsconfig.json",
    "start": "node --require ts-node/register index.ts",
    "start:inspect": "nodemon --exec \"node --require ts-node/register index.ts\"",
    "test": "pnpm run build && git add index.ts && git commit -m 'Add: column ABCD' && git reset HEAD~",
    "test:inspect": "nodemon --exec \"node --require ts-node/register test.ts\""
  },
  "keywords": [],
  "author": "duncanchiu409",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/node": "^24.2.1",
    "eslint": "^9.27.0",
    "husky": "^9.1.7",
    "lint-staged": "15.5.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "@types/argparse": "^2.0.17",
    "argparser": "^0.2.2",
    "dotenv": "^17.2.1",
    "ollama": "^0.5.17",
    "openai": "^5.12.2",
    "winston": "^3.17.0"
  }
}
